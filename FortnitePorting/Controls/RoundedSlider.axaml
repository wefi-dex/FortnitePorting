<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:FortnitePorting.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="FortnitePorting.Controls.RoundedSlider"
             x:DataType="controls:RoundedSlider">
    <Slider Value="{Binding Value, RelativeSource={RelativeSource AncestorType=controls:RoundedSlider}}" Minimum="0.0"
            Maximum="1.0" TickFrequency="0.1" TickPlacement="BottomRight" />
</UserControl>