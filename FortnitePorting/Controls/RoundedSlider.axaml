<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:FortnitePorting.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="FortnitePorting.Controls.RoundedSlider"
             x:DataType="controls:RoundedSlider">
    <Slider Value="{Binding Value, RelativeSource={RelativeSource AncestorType=controls:RoundedSlider}}" 
            Minimum="{Binding Minimum, RelativeSource={RelativeSource AncestorType=controls:RoundedSlider}}"
            Maximum="{Binding Maximum, RelativeSource={RelativeSource AncestorType=controls:RoundedSlider}}"
            TickFrequency="{Binding TickFrequency, RelativeSource={RelativeSource AncestorType=controls:RoundedSlider}}" 
            TickPlacement="BottomRight" />
</UserControl>